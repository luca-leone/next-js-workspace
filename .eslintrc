{
	"extends": [
		"eslint:recommended",
		"google"
	],
	"env": {
		"browser": true,
		"node": true
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 7,
		"sourceType": "module"
	},
	"rules": {
		"no-undef": 0,
		"no-console": 0,
		"object-curly-spacing": "off",
		"new-cap": "off",
		"require-jsdoc": "warn"		
	}
}